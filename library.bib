file = {:C$\backslash$:/Users/SCHMIDT/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Harbst et al. - 2012 - Molecular profiling reveals low- and high-grade forms of primary melanoma.pdf:pdf},
issn = {10780432},
