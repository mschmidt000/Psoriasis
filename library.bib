file = {:C$\backslash$:/Users/SCHMIDT/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nieto et al. - 2020 - A single-cell tumor immune atlas for precision oncology.pdf:pdf},
issn = {26928205},
keywords = {Cancer,Digital pathology,Immuno-oncology,Immuno-therapy,RNA sequencing,Single-cell genomics,Spatial transcriptomics,Transcriptomics,Tumor microenvironment},
month = {oct},
pages = {2020.10.26.354829},
publisher = {bioRxiv},
title = {{A single-cell tumor immune atlas for precision oncology}},
