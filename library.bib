file = {:C$\backslash$:/Users/SCHMIDT/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Joost et al. - 2018 - Single-Cell Transcriptomics of Traced Epidermal and Hair Follicle Stem Cells Reveals Rapid Adaptations during W(2).pdf:pdf},
issn = {22111247},
journal = {Cell Rep.},
keywords = {Lgr5 stem cells,Lgr6 stem cells,RNA sequencing,cellular plasticity,computational analysis,lineage tracing,mouse skin,receptor-ligand pairing,transcriptional adaptation,wound healing},
