keywords = {ANOVA,C-reactive protein,CRP,CT,DMARD,DMT,GNG10,HAQ,HLA-DQB1,Health Assessment Questionnaire,analysis of variance,data mining tool,disease-modifying antirheumatic drug,guanine nucleotide binding protein 10,threshold cycle},
number = {2-3},
