file = {:C$\backslash$:/Users/SCHMIDT/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Joost et al. - 2016 - Single-Cell Transcriptomics Reveals that Differentiation and Spatial Signatures Shape Epidermal and Hair Follicle.pdf:pdf},
issn = {24054720},
journal = {Cell Syst.},
month = {sep},
number = {3},
pages = {221--237.e9},
pmid = {27641957},
publisher = {Cell Press},
title = {{Single-Cell Transcriptomics Reveals that Differentiation and Spatial Signatures Shape Epidermal and Hair Follicle Heterogeneity}},
url = {http://dx.doi.org/10.1016/j.cels.2016.08.010http://dx.doi.org/10.1016/j.cels.2016.08.010},
