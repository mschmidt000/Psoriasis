journal = {Hortic. Res.},
keywords = {Jason Sawler,MEDLINE,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,PMC5539807,PubMed Abstract,Sean Myles,Zo{\"{e}} Migicovsky,doi:10.1038/hortres.2017.35,pmid:28791127},
month = {aug},
publisher = {Nature Publishing Group},
title = {{Patterns of genomic and phenomic diversity in wine and table grapes}},
url = {https://pubmed.ncbi.nlm.nih.gov/28791127/},
volume = {4},
year = {2017}
}
