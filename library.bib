journal = {Biomark. Med.},
keywords = {Alzheimer Disease,Alzheimer Disease: blood,Alzheimer Disease: genetics,Alzheimer Disease: metabolism,Animals,Biological Markers,Biological Markers: blood,Biological Markers: metabolism,Consensus,Humans,Proteomics,Transcriptome},
