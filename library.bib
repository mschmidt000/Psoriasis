publisher = {Oxford University Press (OUP)},
title = {{Surveying brain tumor heterogeneity by single-cell RNA-sequencing of multi-sector biopsies}},
url = {https://academic.oup.com/nsr/article/7/8/1306/5849029},
volume = {7},
