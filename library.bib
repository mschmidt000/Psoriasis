journal = {Radiat. Res.},
keywords = {Aged,Aged, 80 and over,Female,Gamma Rays,Gene Expression,Genome, Human,Humans,Male,MicroRNAs,Middle Aged,Occupational Exposure,Plutonium,RNA, Messenger,Transcriptome,adverse effects,blood,radiation effects},
number = {3},
pages = {299--309},
pmid = {25076116},
title = {{Independent validation of candidate genes identified after a whole genome screening on Mayak workers exposed to prolonged occupational radiation.}},
volume = {182},
year = {2014}
