keywords = {Animals,Cell Differentiation / genetics*,Cell Lineage / genetics*,Computational Biology,Developmental,Florian Buettner,Gene Expression Regulation,Genetic Heterogeneity*,Kedar N Natarajan,MEDLINE,Mice,Models,Mouse Embryonic Stem Cells,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Non-U.S. Gov't,Oliver Stegle,PubMed Abstract,RNA,RNA / genetics,Research Support,Sequence Analysis,Single-Cell Analysis,Th2 Cells / cytology*,Theoretical,Transcriptome / genetics,doi:10.1038/nbt.3102,pmid:25599176},
number = {2},
